{
  "name": "@kokoa-devs/kokoa-components",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.1",
  "description": "React component library scaffolded with Tailwind CSS styling.",
  "author": "",
  "license": "MIT",
  "main": "./dist/kokoa-components.cjs.js",
  "module": "./dist/kokoa-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kokoa-components.es.js",
      "require": "./dist/kokoa-components.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "tsc --project tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/react-vite": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.17",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "storybook": "^8.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.0.11",
    "vitest": "^1.6.0"
  }
}
